{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "LicenseManager"
    }
  },
  "DatabaseConfiguration": {
    "ConnectionString": "Server=licensemanager_db;Port=5432;Database=master;User Id=admin;Password=Admin123!"
  },
  "LicenseAssignmentCleanupOptions": {
    "CleanupFrequencyInDays": 7
  },
  "LicenseReservationCleanupOptions": {
    "CleanupFrequencyInHours": 1
  }
}